<fieldset class="optional prompt">
  <legend>Additional Information</legend>
  <%= f.input :contributor,             as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :date_created,            as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :subject,                as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :publisher,               as: :multi_value, label: "Publisher (Required for DOI)", input_html: { class: 'input-xlarge', id: 'publisher' } %>
  <%= f.input :recommended_citation, as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :language,             as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :source,                as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :temporal_coverage,       as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :spatial_coverage,       as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :repository_name,        as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :collection_name,         as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :methodology,             input_html: { class: 'input-xlarge' } %>
  <%= f.input :data_processing,         input_html: { class: 'input-xlarge' } %>
  <%= f.input :variable_list,           input_html: { class: 'input-xlarge' } %>
  <%= f.input :code_list,               input_html: { class: 'input-xlarge' } %>
  <%= f.input :size,                    as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :requires,                as: :multi_value, input_html: { class: 'input-xlarge' } %>
  <%= f.input :contributor_institution, as: :multi_value, input_html: { class: 'input-xlarge' } %>
</fieldset>
